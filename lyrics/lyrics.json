[
  { "start": 0.0, "text": "（イントロ）" },
  { "start": 3.0, "text": "運命的に出会った　僕らゆだてファミリー" },
  { "start": 12.1, "text": "最高の同志　繋いだ手を離さない絶対に" },
  { "start": 21.2, "text": "苦しい時も、悲しい時も" },
  { "start": 24.8, "text": "みんなが信じてる　あなたも信じて　自分自身を" },
{ "start": 31.3, "text": "この絆は何よりも強く結ばれている" },
{ "start": 41.3, "text": "誰でもできることを" },
{ "start": 45.2, "text": "誰もできないくらいやってみよう" },
{ "start": 49.0, "text": "続かなくても大丈夫" },
{ "start": 54.3, "text": "また始めればいい" },
{ "start": 58.6, "text": "誰でもできることを" },
{ "start": 62.9, "text": "誰もできないくらいやってみようよ" },
{ "start": 67.1, "text": "コツコツが勝つコツだよ" },
{ "start": 69.5, "text": "ゆだてリーダー" },
 { "start": 71.3, "text": "心からありがとう" },
{ "start": 75.5, "text": "みんなで掴もう　夢は叶う" }
]
